<?xml version='1.0' encoding='utf-8'?>
<widget id="org.cesarzagonel.cordova_web_bootloader" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova Web Bootloader</name>
    <description>
        Simple bootloader template that loads your application from web.
    </description>
    <author email="zagonelcesar@gmail.com" href="https://github.com/cesarzagonel">
        Cesar Zagonel
    </author>
    <content src="https://cesarzagonel.github.io/vue-webpack-material-template/" />
    <allow-navigation href="https://cesarzagonel.github.io/vue-webpack-material-template/*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="OverrideUserAgent" value="CordovaAndroid/1.0.0" />
        <preference name="AndroidExtraFilesystems" value="assets" />
        <preference delete="true" name="android-manifest/uses-permission[@android:name='android.permission.WRITE_EXTERNAL_STORAGE']" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="OverrideUserAgent" value="CordovaiOS/1.0.0" />
        <preference name="iosExtraFilesystems" value="bundle" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-custom-config" spec="^4.0.2" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
